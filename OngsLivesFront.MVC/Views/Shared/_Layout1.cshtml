<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - OngsLives</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    @*<link rel="stylesheet" href="https://bootswatch.com/5/vapor/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/OngsLivesFront.MVC.styles.css" asp-append-version="true" />


<style>
  @@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400&display=swap');

*{
    margin: 0;
    padding: 0;
}

body{

    font-family: 'Open Sans', sans-serif;

    color: black;
    font-size: 14px;
    
    display: grid;
    grid-template-columns: 1fr;
    gap: 20px;

     background: linear-gradient(68.15deg, #d37f8d 16.62%, #e65f75 85.61%);
}

a{
    text-decoration: none;
}

header{
    width: 100%;
    height: 33px;
    display: flex;
    align-items: center;
    text-align: center;
    justify-content: space-around;
    background-color: #BD4D9E;
    padding: 7px;    
    box-sizing: border-box;
}

.cabeca {
    padding: 10px;
}

.cabecaEsquerda{
    height: max-content;
    width: max-content;
}

.cabecaEsquerda a{
/*    background-color: #a9fbb1;  */
    text-decoration: none;
    padding: 8.5px;
    padding-right: 15px;
    margin-left: 10px;
    color: black;    

    text-align: center;

    border-radius: 4px;
    margin: 5px;
    padding: 5px 10px;
}

.cabecaDireita{
    align-items: center;
    text-align: center;
}

.cabecaDireita a {
    text-align: center;
    border-radius: 4px;
    margin: 5px;
    padding: 5px 10px;
}

.cabecaDireita .lupa 
{
    padding: 5px;
    border-radius: 50%;
    background-color: #BD4D9E;
}

.centralCliente{
    color: black;
   /* background-color: #a9fbb1;*/
    text-decoration: none;
    padding: 5px;
    padding-left: 20px;
    margin-right: 30px;
    border-radius: 5px;        
}

.cabecaDireita i{
    background-color: black;
    color: white;
    padding: 9px;
    padding-top: 10px;    
}

.linha2{
    display: flex;
    text-align: center;
    justify-content: center;
    align-items: center;

    margin-top: 40px;
    margin-bottom: 20px;
 
    gap:20px;
}


.suporte, .mensagem, .chat{
    width: 150px;
    height: 20px;
    
    padding: 20px 5px;

    background-color: #a9fbb1;
    color: #000;

    display: flex;
    justify-content: center;
    align-items: center;

    text-align: center;  
    align-items: center;

    border-radius: 4px;

}

.suporte, .mensagem, .chat a{
    font-size: 0.9rem;

}

.opcoes {
    margin-left: 5px;
    margin-right: 5px; 
}


.chat{

}

.l1{
    display: flex;
    justify-content: center;   
}

.l1 hr{
    width: 950px;
    box-sizing: border-box;
}

.linha3{
    display: flex;
    text-align: center;
    justify-content: center;
    gap: 60px;
    margin-top: 5px;
    margin-bottom: 5px;
    position: relative;

    background: #BD4D9E;
    color: #fff;
    font-weight: bold;
    padding:5px;
}

.linha3 a{
    background: #BD4D9E;
    color: #fff;
}

.listradinamica1 {
    position: absolute;
    top: -8px;

    width: 100vw;
    height: 30px;
    background-color: rgba(255,255,255, 0);
    z-index: -1;
}

.listradinamica2 {
    position: absolute;
    top: 20px;

    background-color: rgba(0,0,0,0.5);
}

.corpo{

    background-color: #E45665;
}

.linha3 i {
    display: none;
}

.pe {
    width: 100vw;
    height: 33px;
    background-color: #BD4D9E;
    padding: 7px;
    box-sizing: border-box;
    position: absolute;
    bottom: 0px;
}

.caixa-gif {
    display: flex;
    justify-content: center;
    align-items: center;
}

.background-gif {
   width: 300px;
   border-radius: 10px;
   border: 5px solid rgba(255,255,255,0.4);
}



@@media (max-width: 800px)
{
    .corpo{
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center;
    }

    .listra1, .listra2 {
        display: none;
    }

    .fa-chevron-down {display: none;}

    .opcoes {
        display: none;
    }

    .suporte, .mensagem, .chat{
        width: 100px;
        height: auto;
        padding: 10px 5px;
        background-color: #a9fbb1;
        text-align: center;  
        align-items: center;
    
        border-radius: 4px;
    
    }

    .linha3{
        display: flex;
        text-align: center;
        justify-content: center;
        gap: 20px;
        margin-top: 0px;
        margin-bottom: 0px;

        position: relative;
    }

    .listradinamica1 {
        display: none;
    }

    .listradinamica2 {
                display: none;
    }

    .linha3 a {
        font-size: 0.8rem;
    }

    
    .opcao-menu {
        display: block;
    }

    .caixa-gif{
        display: flex;
        justify-content: center;
        align-items: center;
     }

    .background-gif {

    }

}

@@media (max-width: 500px)
{
    body {
    margin-bottom: 10px;
    height: 50vh;
}

    .linha2 {
        margin-top: 10px;
        margin-bottom: 20px;
    }

    .linha3 a {
        font-size: 0.6rem;
        border: 0px;
    }

    .suporte, .mensagem, .chat{
        width: 20vw;
        height: 10vh;
        background-color: #a9fbb1;
        text-align: center;  
        align-items: center;
    }

    .suporte, .mensagem, .chat a 
    {
        font-size: 20px;
        
    }


        .cabeca a{
        align-self:center;
    }

    .cabeca {
        display: flex;
        flex-direction: row;
        justify-content: space-between;
    }

    .cabeca .cabecaDireita {
    }

    .corpo{
        background-size: cover;
        background-repeat: no-repeat;
        background-position: center;
        filter: blur(4px);
        z-index: 0;
    }

    
    .pe{
        width: 100vw;
        height: 33px;
        background-color: #BD4D9E;
        padding: 7px;    
        box-sizing: border-box;
        position: absolute;
        bottom: 0px;
    }


    .linha3{
        display: flex;

        justify-content: center;
        align-items: center;

        gap: 10px;
        margin-top: 0px;
        margin-bottom: 0px;
        position: relative;
        z-index: 1;
        background: rgba(255,255,255,0.1);
    }

            .listradinamica1 {
                position: absolute;
                top: -8px;
                width: 100vw;
                height: 30px;
                background-color: rgba(255,255,255);
                display:none;
            }

            .listradinamica2 {
                position: absolute;
                top: 20px;
                width: 0vw;
                height: 1px;
                background-color: #3d3c3c;
                display: none;
            }

    .linha3 a {
        width: 80px;
        height: 80px;
        border-radius: 50%;
        position: absolute;

        color: #fff;
        
    }

    .linha3 i {
        display: block;
        font-size: 30px;
        text-align: center;
        margin-top: 25px;
    }

        
    .opcao-menu {
        display: none;
    }


    .linha3 a {
        background-color: #f7c5e0;
        /*              background-color: #c2fbd7;*/
        border-radius: 100px;
        box-shadow: rgba(255, 20, 99, .2) 0 -25px 18px -14px inset,rgba(255, 20, 99, .15) 0 1px 2px,rgba(255, 20, 99, .15) 0 2px 4px,rgba(255, 20, 99, .15) 0 4px 8px,rgba(255, 20, 99, .15) 0 8px 16px,rgba(255, 20, 99, .15) 0 16px 32px;
        color: #BD4D9E;
        cursor: pointer;
        text-align: center;
        text-decoration: none;
        transition: all 250ms;
        border: 0;
        font-size: 16px;
        user-select: none;
        -webkit-user-select: none;
        touch-action: manipulation;
    }

    .linha3 a:hover {
        box-shadow: rgba(255,20,99,.35) 0 -25px 18px -14px inset,rgba(255,20,99,.25) 0 1px 2px,rgba(255,20,99,.25) 0 2px 4px,rgba(255,20,99,.25) 0 4px 8px,rgba(255,20,99,.25) 0 8px 16px,rgba(255,20,99,.25) 0 16px 32px;
        transform: scale(1.05) rotate(-1deg);
    }


    .linha3 a:nth-child(2) 
    {
        top: 100px;
        left: 15vw;
    }

    .linha3 a:nth-child(3) 
    {
        top: 100px;
        right: 15vw;
     /*   background-color: #BD4D9E;*/
    }

    .linha3 a:nth-child(4) 
    {
        top: 200px;
        left: 15vw;
        /*background-color: #BD4D9E;*/
    }
    
    .linha3 a:nth-child(5) 
    {
        top: 200px;
        right: 15vw;
        /*background-color: #BD4D9E;*/
    }

    .linha3 a:nth-child(6) 
    {
        top: 300px;
        left: 15vw;
        /*background-color: #BD4D9E;*/
    }

    .linha3 a:nth-child(7) 
    {
        top: 300px;
        right: 15vw;
        /*background-color: #BD4D9E;*/
    }

    .linha3 a:nth-child(8) {
        top: 0px;
        right: 38vw;
        /*background-color: #BD4D9E;*/
    }

    .background-gif {
        display: none;
        
    }

}

       #loading-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.7);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9999;
}

#loading-spinner {
  width: 40px;
  height: 40px;
  border: 4px solid #fff;
  border-top-color: transparent;
  border-radius: 50%;
  animation: spin 5s infinite linear;
}

@@keyframes spin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}


        @@media (max-height: 700px) and (max-width: 400px){
            body{

                color: black;
                font-size: 14px;
    
                display: grid;
                grid-template-columns: 1fr;
                gap: 0px;

                 background: linear-gradient(68.15deg, #d37f8d 16.62%, #e65f75 85.61%);
            }

            .pe {
                height: 10px;
             
            }
        }




        .button-33 {
            background-color: #c2fbd7;
            border-radius: 100px;
            box-shadow: rgba(44, 187, 99, .2) 0 -25px 18px -14px inset,rgba(44, 187, 99, .15) 0 1px 2px,rgba(44, 187, 99, .15) 0 2px 4px,rgba(44, 187, 99, .15) 0 4px 8px,rgba(44, 187, 99, .15) 0 8px 16px,rgba(44, 187, 99, .15) 0 16px 32px;
            color: green;
            cursor: pointer;
            text-align: center;
            text-decoration: none;
            transition: all 250ms;
            border: 0;
            font-size: 16px;
            user-select: none;
            -webkit-user-select: none;
            touch-action: manipulation;
        }

        .button-33:hover {
            box-shadow: rgba(44,187,99,.35) 0 -25px 18px -14px inset,rgba(44,187,99,.25) 0 1px 2px,rgba(44,187,99,.25) 0 2px 4px,rgba(44,187,99,.25) 0 4px 8px,rgba(44,187,99,.25) 0 8px 16px,rgba(44,187,99,.25) 0 16px 32px;
            transform: scale(1.05) rotate(-1deg);
        }

</style>

</head>


<body>

    <div id="loading-overlay">
        <div id="loading-spinner"></div>
    </div>

    @await Component.InvokeAsync("MenuCima")
    <div class="linha2">

        <div class="suporte button-33">
            <i class="fa-solid fa-file-pen"></i>
            <a asp-controller="Suporte" asp-action="Index" target="_blank" class="opcoes" style="color:#000"> SUPORTE</a>
        </div>

        <div class="mensagem button-33">
            <i class="fa-solid fa-bell"></i>
            <a href="" class="opcoes" style="color:#000"> MENSAGENS</a>
        </div>

        <div class="chat button-33">
            <i class="fa-solid fa-comments"></i>
            <a href="" class="opcoes" style="color:#000"> CHAT</a>
        </div>

    </div>

    @await Component.InvokeAsync("MenuBaixo")

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="pe">
    </footer>

     <script src="https://kit.fontawesome.com/c2daddfa23.js" crossorigin="anonymous"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
